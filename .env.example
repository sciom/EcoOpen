## Copy this file to .env and fill in values.
# Do not commit your real .env with secrets.

# Agent / LLM (OpenAI-compatible or Ollama)
# Examples:
# AGENT_BASE_URL=http://localhost:11434 or https://your-endpoint/v1
AGENT_BASE_URL=
AGENT_MODEL=
AGENT_API_KEY=
AGENT_TIMEOUT_SECONDS=

# Local embeddings / Ollama
OLLAMA_HOST=
OLLAMA_MODEL=
OLLAMA_EMBED_MODEL=

# App & Extraction Repair
MAX_FILE_SIZE_MB=
# JSON array or comma-separated allowed origins for CORS, e.g. ["http://localhost:5173","http://127.0.0.1:5173"]
CORS_ORIGINS=
LOG_LEVEL=

# Repair toggles
REPAIR_CONTEXT_ENABLED=
REPAIR_URLS_ENABLED=

# ChromaDB persistence directory
CHROMA_DB_PATH=

# MongoDB (optional for batch/jobs)
MONGO_URI=
MONGO_DB_NAME=

# Worker concurrency (Mongo-based background worker)
QUEUE_CONCURRENCY=

# Auth (JWT)
# Change this in production and keep it secret.
JWT_SECRET=
JWT_ALGORITHM=
JWT_EXPIRES_MINUTES=

# MCP (optional)
MCP_ENABLED=
MCP_SERVER_URL=
MCP_TOOL_NAME=
